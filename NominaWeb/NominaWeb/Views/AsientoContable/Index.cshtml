            @Html.DisplayFor(modelItem => item.MontoAsiento)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Estado)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Empleados.Cedula)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.IdAsiento }) |
